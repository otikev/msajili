<%= form_for @agent, url: agentsignin_path, html: {class: "form-signin",role:"form"} do |f| %>
    <div class="panel panel-default">
      <div class="panel-heading">
        <b>Agent Sign In</b>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-sm-12">
            <%= f.text_field :email,class:"form-control",placeholder:"Email address", :autofocus=>true, :required=>true %>
          </div>
        </div>
        <div class="row">
          <div class="col-sm-12">
            <%= f.password_field :password, class:"form-control",placeholder:"Password", :required=>true %>
          </div>
        </div>
        <%= f.submit 'Sign in', class: 'btn btn-lg btn-primary btn-block' %>
      </div>
    </div>
<% end %>